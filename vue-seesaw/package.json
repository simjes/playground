{
  "name": "vue-seesaw",
  "version": "1.0.0",
  "description": "Vue-seesaw",
  "author": "Simon Jespersen",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "graphql:download": "apollo schema:download --endpoint=http://localhost:9000/graphql graphql-schema.json",
    "graphql:generate": "apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=graphql/**/*.ts --tagName=gql --addTypename types"
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.0-rc6",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/vuetify": "0.5.5",
    "apollo-boost": "^0.4.2",
    "graphql": "14.2.1",
    "graphql-tag": "^2.10.1",
    "nuxt": "^2.0.0",
    "ts-node": "^8.2.0",
    "vue-apollo": "^3.0.0-beta.30",
    "vue-property-decorator": "^8.1.1"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/eslint-config": "0.0.1",
    "@types/graphql": "14.2.1",
    "@types/node": "^12.0.8",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "apollo": "^2.13.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "resolutions": {
    "graphql": "14.2.1",
    "browserslist": "4.6.2",
    "caniuse-lite": "1.0.30000974"
  }
